[aws_ssm]
{{ windows_output.instances[0].instance_id }} ansible_aws_ssm_instance_id={{ windows_output.instances[0].instance_id }} ansible_aws_ssm_region={{ aws_regioni }}

[aws_ssm:vars]
ansible_shell_type=powershell
ansible_connection=aws_ssm
ansible_aws_ssm_bucket_name={{s3_output.name}}
ansible_aws_ssm_plugin=/usr/local/sessionmanagerplugin/bin/session-manager-plugin

# support tests that target testhost
[testhost:children]
aws_ssm
