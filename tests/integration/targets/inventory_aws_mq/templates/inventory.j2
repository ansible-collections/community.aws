plugin: community.aws.aws_mq
access_key: '{{ aws_access_key }}'
secret_key: '{{ aws_secret_key }}'
{% if security_token | default(false) %}
session_token: '{{ security_token }}'
{% endif %}
regions:
  - '{{ aws_region }}'
{% if inventory_statuses | default(false) %}
statuses:
  - CREATION_FAILED
{% endif %}
